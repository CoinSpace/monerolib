{
  "name": "monerolib",
  "version": "1.1.5",
  "description": "Monero JavaScript library",
  "author": "Coin Crypto Wallet <support@coin.space> (https://coin.space/)",
  "license": "MIT",
  "main": "index.js",
  "type": "module",
  "keywords": [
    "monero",
    "xmr",
    "crypto"
  ],
  "scripts": {
    "test": "mocha",
    "lint": "eslint index.js lib/*.js test/*.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/CoinSpace/monerolib.git"
  },
  "dependencies": {
    "base58-monero": "0.0.5",
    "bn.js": "^4.12.0",
    "buffer": "^6.0.3",
    "elliptic": "^6.5.4",
    "keccak": "^3.0.1",
    "randombytes": "^2.1.0",
    "varint": "^6.0.0"
  },
  "devDependencies": {
    "eslint": "^8.20.0",
    "eslint-config-coinspace": "^2.0.0",
    "mocha": "^10.0.0"
  },
  "eslintConfig": {
    "extends": [
      "coinspace"
    ],
    "env": {
      "browser": true
    },
    "parserOptions": {
      "requireConfigFile": false,
      "sourceType": "module"
    },
    "overrides": [
      {
        "files": [
          "*.test.js",
          "test/*"
        ],
        "env": {
          "node": true,
          "mocha": true
        }
      }
    ]
  }
}
